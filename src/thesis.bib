@unpublished{hwang2021,
abstract = {We introduce a novel qualitative interview technique, Sketching Dialogue, which incorporates sketching as an integral part of a semi-structured interview process. This approach has been evaluated and validated using two distinct case studies involving 21 participants. In the first case study, we demonstrated the value of Sketching Dialogue as an additional supportive artefact for interactive discussion in the interview setting. The second case study involved a process of other researchers applying the method, within six different studies researching a range of topics. We outline how Sketching Dialogue provides a more effective form of engagement with participants, especially in respect of sensitive topics, or where emphatic engagement is required. Through this work, we demonstrate how the use of sketching can enhance the quality of traditional research interviews for HCI, in part due to its particularly flexible and accessible nature. Based on our experience, we also reflect upon the steps and practices needed to introduce a new qualitative research method to HCI researchers and interaction designers.},
author = {Hwang, Euijin},
file = {:Users/b6069620/OneDrive - Newcastle University/Sketch Based Interviews/files from euijin/Sketching Dialogue for IJHCS.pdf:pdf},
mendeley-groups = {OpenLab/!!!thesis/chapter 3},
