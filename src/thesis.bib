booktitle = {Choice Reviews Online},
doi = {10.5860/choice.50-2168},
issn = {0009-4978},
mendeley-groups = {OpenLab/power balance and control,OpenLab/!!!read next,OpenLab/2019-empowerment-tech- !!!!Outline Framing Doc,OpenLab/empowerment},
