keywords = {Client-centered care,Family planning counseling,Job aids,Shared decision-making},
mendeley-groups = {OpenLab/power balance / locus of power,OpenLab/CHI2019-old,OpenLab/involvement},
number = {3},
pages = {355--361},
pmid = {21093194},
publisher = {Elsevier Ireland Ltd},
title = {{Towards client-centered counseling: Development and testing of the WHO Decision-Making Tool}},
url = {http://dx.doi.org/10.1016/j.pec.2010.10.011},
volume = {81},
