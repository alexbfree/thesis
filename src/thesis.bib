journal = {Proceedings of the ACM Conference on Computer Supported Cooperative Work, CSCW},
keywords = {Boundary negotiating artifacts,Chronic disease management,Patient-generated data,Patient-provider collaboration,Personal informatics,Self-tracking},
mendeley-groups = {OpenLab/methods - boundary objects and things to think with},
